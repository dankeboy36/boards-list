    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "2.4.7",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.5",
    "semantic-release-vsce": "^5.6.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
